<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<?eclipse version="3.0"?>

<plugin>
   <extension
         id="ExampleViewComponent"
         point="org.protege.editor.core.application.ViewComponent">
      <label
            value="Editor Grafico de Ontologías">
      </label>
      <class
            value="org.protege.owl.examples.tab.ExampleViewComponent">
      </class>
      <category
            value="@org.protege.ontologycategory">
      </category>
   </extension>
   <extension
         id="ExampleWorkspaceTab"
         point="org.protege.editor.core.application.WorkspaceTab">
      <label
            value="Editor Gráfico de Ontologías">
      </label>
      <class
            value="org.protege.owl.examples.tab.ExampleWorkspaceTab">
      </class>
      <index
            value="X">
      </index>
      <editorKitId
            value="OWLEditorKit">
      </editorKitId>
      <defaultViewConfigFileName
            value="Resources/viewconfig-exampleTab.xml">
      </defaultViewConfigFileName>
   </extension>


</plugin>
